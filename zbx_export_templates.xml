<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.2</version>
    <date>2024-05-28T05:23:55Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Windows Failover Cluster</template>
            <name>Template Windows Failover Cluster</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Failover Cluster</name>
                </application>
                <application>
                    <name>Windows Failover Cluster</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Failover Cluster (Nodes)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>cluster.nodes.discovery</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Failover Cluster node state - {#NAME}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>cluster.node.state[{#ID}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Windows Failover Cluster</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{str(Up)}=0</expression>
                                    <name>Failover Cluster node &quot;{#NAME}&quot; is not online</name>
                                    <priority>DISASTER</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Failover Cluster (Core Resources)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>cluster.resources.discovery[core]</key>
                    <delay>1h</delay>
                    <filter>
                        <evaltype>OR</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>^Virtual Machine Configuration|^Virtual Machine</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Failover Cluster core resource state - {#NAME}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>cluster.resource.state[{#ID}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Windows Failover Cluster</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{str(Online)}=0</expression>
                                    <name>Failover Cluster core resource &quot;{#NAME}&quot; is not online</name>
                                    <priority>DISASTER</priority>
                                    <description>Trigger when a core cluster resource is not in the &quot;Online&quot; state.</description>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Failover Cluster (Virtual Machine Configurations)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>cluster.resources.discovery[vmconfig]</key>
                    <status>DISABLED</status>
                    <filter>
                        <evaltype>AND</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>^Virtual Machine Configuration</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Failover Cluster - {#NAME}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>cluster.resource.state[{#ID},vmconfig]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Windows Failover Cluster</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{str(Online)}=0</expression>
                                    <name>Failover Cluster &quot;{#NAME}&quot; is not online</name>
                                    <priority>DISASTER</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Failover Cluster (Virtual Machines)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>cluster.resources.discovery[vm]</key>
                    <filter>
                        <evaltype>AND</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>^Virtual Machine</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>^Virtual Machine Configuration</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>B</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Failover Cluster - {#NAME}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>cluster.resource.state[{#ID},vm]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Windows Failover Cluster</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{str(Online)}=0</expression>
                                    <name>Failover Cluster &quot;{#NAME}&quot; is not online</name>
                                    <priority>DISASTER</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
